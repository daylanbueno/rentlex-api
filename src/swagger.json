    "paths": {
        "/categories": {
            "post": {
              "tags": ["Categories"],
              "summary": "Create a category",
              "description": "create a new category",
              "requestBody": {
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "description": {
                                    "type": "string"
                                }
                            },
                            "example": {
                              "name": "SUV",
                              "description": "Carro de categoria m√©dia"
                            }
                        }
                    }
                }
              },
             "responses": {
                "201": {
                    "description": "Category created successfully"
                },
                "500": {
                  "description": "Category already exists"
                }
             }
            },
            "get": {
                "tags":["Categories"],
                "summary": "List all categories",
                "description": "List all categories",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "properties": {
                                        "name": {
                                            "type": "string"
                                        },
                                        "description": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }

                        }
                    }
                }
            }
        }
    }
}
